{
    "name": "Camouflaging webpage",
    "description": "Camouflaging the webpage which like Youtube,X and Google",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": ["https://*/*"],
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
    
}